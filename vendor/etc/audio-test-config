#capture_main-mic
#enable_main-mic
enable
TX DEC2 MUX:SWR_MIC
TX SMIC MUX2:SWR_MIC4
TX_AIF1_CAP Mixer DEC2:1
ADC1 ChMap:SWRM_TX2_CH1
TX0 MODE:ADC_LP
ADC1_MIXER Switch:1
AMIC1_MIXER Switch:1

#disable_main-mic
disable
AMIC1_MIXER Switch:0
ADC1_MIXER Switch:1
TX0 MODE:ADC_LP
ADC1 ChMap:SWRM_TX2_CH1
TX_AIF1_CAP Mixer DEC2:0
TX SMIC MUX2:ZERO
TX DEC2 MUX:MSM_DMIC

#capture_top-mic
#enable_top-mic
enable
TX DEC3 MUX:SWR_MIC
TX SMIC MUX3:SWR_MIC9
TX_AIF1_CAP Mixer DEC3:1
ADC4 MUX:INP5
ADC4 ChMap:SWRM_TX3_CH2
TX3 MODE:ADC_LP
ADC4_MIXER Switch:1
AMIC5_MIXER Switch:1

#disable_top-mic
disable
AMIC5_MIXER Switch:0
ADC4_MIXER Switch:1
TX3 MODE:ADC_LP
ADC4 ChMap:SWRM_TX3_CH2
ADC4 MUX:INP5
TX_AIF1_CAP Mixer DEC3:0
TX SMIC MUX3:ZERO
TX DEC3 MUX:MSM_DMIC

#capture_back-mic
#enable_back-mic
enable
TX DEC5 MUX:SWR_MIC
TX SMIC MUX5:SWR_MIC8
TX_AIF1_CAP Mixer DEC5:1
ADC3 MUX:INP4
ADC3 ChMap:SWRM_TX3_CH1
TX2 MODE:ADC_LP
HDR34 MUX:NO_HDR34
ADC3_MIXER Switch:1
AMIC4_MIXER Switch:1

#disable_back-mic
disable
AMIC4_MIXER Switch:0
ADC3_MIXER Switch:0
HDR34 MUX:NO_HDR34
TX2 MODE:ADC_LP
ADC3 ChMap:SWRM_TX3_CH1
ADC3 MUX:INP4
TX_AIF1_CAP Mixer DEC5:0
TX SMIC MUX5:ZERO
TX DEC5 MUX:MSM_DMIC

#capture_us
enable
TX DEC4 MUX:SWR_MIC
TX SMIC MUX4:SWR_MIC9
TX_AIF2_CAP Mixer DEC4:1
ADC3 ChMap:SWRM_TX3_CH2
HDR34 MUX:HDR34
TX2 MODE:ADC_LP
ADC3_MIXER Switch:1
ADC3 Volume:12
AMIC5_MIXER Switch:1

disable
HDR34 MUX:NO_HDR34
ADC3_MIXER Switch:0
TX2 MODE:ADC_LP
ADC3 ChMap:ZERO
TX_AIF2_CAP Mixer DEC4:0
TX SMIC MUX4:ZERO
ADC3 Volume:6


