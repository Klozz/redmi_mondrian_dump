HPHR_RDAC Switch:1
disable

#teardown_loopback
enable
HDR12 MUX:NO_HDR12
ADC1 Volume:6
ADC2 Volume:6
ADC3 Volume:6
ADC4 Volume:6
AMIC1_MIXER Switch:0
AMIC2_MIXER Switch:0
AMIC3_MIXER Switch:0
AMIC4_MIXER Switch:0
AMIC5_MIXER Switch:0
ADC1_MIXER Switch:1
ADC2_MIXER Switch:0
ADC3_MIXER Switch:0
ADC4_MIXER Switch:1
TX0 MODE:ADC_NORMAL
TX1 MODE:ADC_NORMAL
TX2 MODE:ADC_NORMAL
TX3 MODE:ADC_NORMAL
ADC2 MUX:INP2
ADC4 MUX:INP5
ADC1 ChMap:SWRM_TX2_CH1
ADC2 ChMap:ZERO
ADC3 ChMap:ZERO
ADC4 ChMap:SWRM_TX3_CH2
TX_AIF1_CAP Mixer DEC0:0
TX_AIF1_CAP Mixer DEC1:0
TX_AIF1_CAP Mixer DEC2:0
TX_AIF1_CAP Mixer DEC3:0
TX_AIF2_CAP Mixer DEC0:0
TX_AIF2_CAP Mixer DEC3:0
TX_AIF2_CAP Mixer DEC4:0
TX SMIC MUX0:ZERO
TX SMIC MUX1:ZERO
TX SMIC MUX2:ZERO
TX SMIC MUX3:ZERO
TX SMIC MUX4:ZERO
TX DEC0 MUX:ZERO
TX DEC1 MUX:ZERO
TX DEC4 MUX:ZERO
TX DEC1 MUX:MSM_DMIC
TX DEC2 MUX:MSM_DMIC
TX DEC3 MUX:MSM_DMIC
TX DEC4 MUX:MSM_DMIC
TX DEC5 MUX:MSM_DMIC
BCS Channel:0
MBHC ChMap:ZERO
BCS Channel:CH0
BCS CH_SEL:SWR_MIC6
RX INT0_1 MIX1 INP0:ZERO
RX INT1_1 MIX1 INP0:ZERO
RX INT0 DEM MUX:NORMAL_DSM_OUT
RX INT1 DEM MUX:NORMAL_DSM_OUT
RX_HPH_PWR_MODE:ULP
RX HPH Mode:CLS_H_ULP
RX_COMP1 Switch:0
RX_COMP2 Switch:0
HPHL_COMP Switch:0
HPHR_COMP Switch:0
HPHL Volume:18
HPHR Volume:18
HPHL_RDAC Switch:0
HPHR_RDAC Switch:0
aw_dev_0_prof:Music
aw_dev_1_prof:Music
aw_dev_0_switch:Disable
aw_dev_1_switch:Disable
aw882xx_spk_sw_gpio_switch:0
disable


#enable_ultrasound_mic
enable
